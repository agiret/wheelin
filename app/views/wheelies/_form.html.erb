<%= simple_form_for(@wheely) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.association :category %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :price %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
